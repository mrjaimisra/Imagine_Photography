<div class="row photo-list">
  <% @photos.each do |photo| %>
    <div class="col-sm-3 col-lg-3 col-md-3 item-box">
      <div class="thumbnail">
        <a class="photo" href="<%= photo_path(photo) %>"><%= image_tag photo.image.url %></a>
      </div> <!-- ./thumbnail -->
    </div>
  <% end %>
</div>
